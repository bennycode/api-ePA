<?xml version="1.0" encoding="UTF-8"?>
<ValueSet xmlns="http://hl7.org/fhir">
    <id value="author-roles-phr-system"/>
    <meta>
        <profile value="http://hl7.org/fhir/StructureDefinition/shareablevalueset"/>
    </meta>
    <extension url="http://hl7.org/fhir/StructureDefinition/resource-effectivePeriod">
        <valuePeriod>
            <start value="2022-03-31"/>
        </valuePeriod>
    </extension>
    <url value="https://gematik.de/fhir/ValueSet/author-roles-phr-system"/>
    <identifier>
        <system value="urn:ietf:rfc:3986"/>
        <value value="urn:oid:1.2.276.0.76.11.579"/>
    </identifier>
    <version value="1.0.0"/>
    <name value="value-set-of-author-roles"/>
    <title value="Value Set of Author Roles"/>
    <status value="active"/>
    <experimental value="false"/>
    <publisher value="gematik GmbH"/>
    <contact>
        <telecom>
            <system value="url"/>
            <value value="http://www.gematik.de"/>
        </telecom>
    </contact>
    <description value="Rolle des Autors des Dokuments für ePA 1.0 und ePA 2.0"/>
    <compose>
        <include>
            <extension url="http://hl7.org/fhir/StructureDefinition/valueset-systemName">
                <valueString value="Prozessrollen für Autoren"/>
            </extension>
            <system value="urn:oid:1.3.6.1.4.1.19376.3.276.1.5.13"/>
            <concept>
                <extension url="http://hl7.org/fhir/StructureDefinition/valueset-concept-comments">
                    <valueString value="Die Person, die die Einweisung veranlasst hat. (Sie wird auch als Zuweiser bezeichnet.)"/>
                </extension>
                <code value="1"/>
                <display value="Einweiser"/>
            </concept>
            <concept>
                <extension url="http://hl7.org/fhir/StructureDefinition/valueset-concept-comments">
                    <valueString value="Die Person, die für die Entlassung verantwortlich ist."/>
                </extension>
                <code value="2"/>
                <display value="Entlassender"/>
            </concept>
            <concept>
                <extension url="http://hl7.org/fhir/StructureDefinition/valueset-concept-comments">
                    <valueString value="Die Person, die die Überweisung veranlasst hat."/>
                </extension>
                <code value="3"/>
                <display value="Überweiser"/>
            </concept>
            <concept>
                <extension url="http://hl7.org/fhir/StructureDefinition/valueset-concept-comments">
                    <valueString value="Die Person oder das Gerät, welche(s) die dokumentierte Aktion durchgeführt hat."/>
                </extension>
                <code value="4"/>
                <display value="Durchführender"/>
            </concept>
            <concept>
                <extension url="http://hl7.org/fhir/StructureDefinition/valueset-concept-comments">
                    <valueString value="Das Gerät, welches die dokumentierte Aktion durchgeführt hat."/>
                </extension>
                <code value="5"/>
                <display value="durchführendes Gerät"/>
            </concept>
            <concept>
                <extension url="http://hl7.org/fhir/StructureDefinition/valueset-concept-comments">
                    <valueString value="Die Person, die den Patienten betreut, d.h. den Überblick über die Behandlung behält und als primärer Ansprechpartner während der Behandlung dient."/>
                </extension>
                <code value="6"/>
                <display value="Betreuer"/>
            </concept>
            <concept>
                <extension url="http://hl7.org/fhir/StructureDefinition/valueset-concept-comments">
                    <valueString value="Die Person, die den Patienten pflegerisch (aktiv) betreut."/>
                </extension>
                <code value="7"/>
                <display value="Pflegender"/>
            </concept>
            <concept>
                <extension url="http://hl7.org/fhir/StructureDefinition/valueset-concept-comments">
                    <valueString value="Die Person, die ein Gutachten erstellt."/>
                </extension>
                <code value="17"/>
                <display value="Begutachtender"/>
            </concept>
            <concept>
                <extension url="http://hl7.org/fhir/StructureDefinition/valueset-concept-comments">
                    <valueString value="Die Person, die den Patienten aktiv behandelt im Sinne der Verbesserung (oder zumindest Beibehaltung) des gesundheitlichen Zustandes."/>
                </extension>
                <code value="8"/>
                <display value="Behandler"/>
            </concept>
            <concept>
                <extension url="http://hl7.org/fhir/StructureDefinition/valueset-concept-comments">
                    <valueString value="Die Person, die den Patienten außerhalb der (eigenen) Einrichtung zuerst behandelt hat. Das kann - muss aber nicht - im Rahmen einer Notfallversorgung stattgefunden haben."/>
                </extension>
                <code value="9"/>
                <display value="Erstbehandler außerhalb einer Einrichtung"/>
            </concept>
            <concept>
                <extension url="http://hl7.org/fhir/StructureDefinition/valueset-concept-comments">
                    <valueString value="Die Person, die zusätzliche Mittel (Medikamente, Heil-/Hilfsmittel) bereitgestellt hat."/>
                </extension>
                <code value="10"/>
                <display value="Bereitstellender"/>
            </concept>
            <concept>
                <extension url="http://hl7.org/fhir/StructureDefinition/valueset-concept-comments">
                    <valueString value="Die Person, die übermittelte Informationen eingibt oder ein Gerät, das erzeugte Daten im System hinterlegt."/>
                </extension>
                <code value="11"/>
                <display value="Dokumentierender"/>
            </concept>
            <concept>
                <extension url="http://hl7.org/fhir/StructureDefinition/valueset-concept-comments">
                    <valueString value="Das Gerät, welches erzeugte Daten im System hinterlegt."/>
                </extension>
                <code value="12"/>
                <display value="dokumentierendes Gerät"/>
            </concept>
            <concept>
                <extension url="http://hl7.org/fhir/StructureDefinition/valueset-concept-comments">
                    <valueString value="Die Person, die Korrektheit der hinterlegten Information überprüft und verifiziert hat."/>
                </extension>
                <code value="13"/>
                <display value="Validierer"/>
            </concept>
            <concept>
                <extension url="http://hl7.org/fhir/StructureDefinition/valueset-concept-comments">
                    <valueString value="Die Person, die vor dem Gesetz für die Korrektheit der Informationen verantwortlich ist und dafür haftet."/>
                </extension>
                <code value="14"/>
                <display value="Gesetzlich Verantwortlicher"/>
            </concept>
            <concept>
                <extension url="http://hl7.org/fhir/StructureDefinition/valueset-concept-comments">
                    <valueString value="Die Person, die Auswertungen vorgenommen und beratend involviert war."/>
                </extension>
                <code value="15"/>
                <display value="Beratender"/>
            </concept>
            <concept>
                <extension url="http://hl7.org/fhir/StructureDefinition/valueset-concept-comments">
                    <valueString value="Eine Person, die zu den hier dokumentierten Informationen beigetragen hat."/>
                </extension>
                <code value="16"/>
                <display value="Informierender"/>
            </concept>
        </include>
        <include>
            <extension url="http://hl7.org/fhir/StructureDefinition/valueset-systemName">
                <valueString value="Patientenbeziehungsrollen für Autoren"/>
            </extension>
            <system value="urn:oid:1.3.6.1.4.1.19376.3.276.1.5.14"/>
            <concept>
                <extension url="http://hl7.org/fhir/StructureDefinition/valueset-concept-comments">
                    <valueString value="der Hausarzt (Primärversorger) des Patienten"/>
                </extension>
                <code value="101"/>
                <display value="Hausarzt"/>
            </concept>
            <concept>
                <extension url="http://hl7.org/fhir/StructureDefinition/valueset-concept-comments">
                    <valueString value="der Patient selbst"/>
                </extension>
                <code value="102"/>
                <display value="Patient"/>
            </concept>
            <concept>
                <extension url="http://hl7.org/fhir/StructureDefinition/valueset-concept-comments">
                    <valueString value="ein Vertreter des Arbeitgebers des Patienten"/>
                </extension>
                <code value="103"/>
                <display value="Arbeitgebervertreter"/>
            </concept>
            <concept>
                <extension url="http://hl7.org/fhir/StructureDefinition/valueset-concept-comments">
                    <valueString
                            value="Die Person, die den Patienten normalerweise primär betreut. Beispiele: Langzeitpflege, Stammapotheke"/>
                </extension>
                <code value="104"/>
                <display value="Primärbetreuer (langfristig)"/>
            </concept>
            <concept>
                <extension url="http://hl7.org/fhir/StructureDefinition/valueset-concept-comments">
                    <valueString value="ein Vertreter des Kostenträgers"/>
                </extension>
                <code value="105"/>
                <display value="Kostenträgerverteter"/>
            </concept>
        </include>
    </compose>
</ValueSet>